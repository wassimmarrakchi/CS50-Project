{
  "manifest_version": 2,

  "name": "ProcrastAnki",
  "description": "This extension prevents procrastination in a productive way.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js"]
    }
  ]

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "browser"
  ]
}
